{
  "name": "@virtuous/redux-persister",
  "version": "1.1.0-beta.1",
  "description": "Store enhancer that persists (a subset) of your Redux store locally.",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rm -rf dist/ && NODE_ENV=production babel src --out-dir dist --no-comments",
    "postbuild": "git add -A",
    "preversion": "yarn run build",
    "postversion": "git push && git push --tags",
    "release": "npm publish --access=public",
    "release:beta": "npm publish --access=public --tag=beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/virtuous/redux-persister.git"
  },
  "bugs": {
    "url": "https://github.com/virtuous/redux-persister/issues"
  },
  "homepage": "https://github.com/virtuous/redux-persister#readme",
  "keywords": [
    "redux",
    "localstorage",
    "persist"
  ],
  "contributors": [
    "Florian MÃ¼ller <florian.mueller81@gmx.de> (https://github.com/devbucket)"
  ],
  "devDependencies": {
    "@virtuous/eslint-config": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0"
  },
  "dependencies": {
    "@virtuous/logger": "^1.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-minify-mangle-names": "^0.4.3",
    "greenlet": "^1.0.1",
    "lodash": "^4.17.10"
  }
}
